        <mxCell id="EOWl2WxTxrsTDOCXnR41-15" value="CONSEIL AVEC FARRELL" style="whiteSpace=wrap;html=1;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="1680" y="500" width="120" height="59.38" as="geometry" />
